{
  "presets": [
    ["env"],
    "react",
    "stage-0",
    "stage-2",
    "flow"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-decorators-legacy",
    "flow-react-proptypes",
    "transform-react-jsx",
    "transform-flow-strip-types",
    "transform-object-assign",
    "react-html-attrs",
    "add-module-exports"
  ],
  "env": {
    "test": {
      "presets": [
        "env",
        "react",
        "stage-0",
        "stage-2",
        "flow"
      ]
    }
  }
}
